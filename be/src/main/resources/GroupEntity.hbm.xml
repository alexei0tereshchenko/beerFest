<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="study.beerFest.dao.GroupEntity" table="Group" schema="beerFest">
        <id name="idGroup">
            <column name="idGroup" sql-type="int(11)"/>
        </id>
        <property name="groupName">
            <column name="GroupName" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>